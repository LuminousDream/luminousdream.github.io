<% if(theme.pwa.enable) { %>
<script>
    if ('serviceWorker' in navigator) {
        window.addEventListener('load', function () {
            navigator.serviceWorker.register("/pwa_sw.js")
                .then(function (registration) {
                    console.log('%c PWA Service Worker 注册成功 ',"margin:5px;color:#F05B85;padding:5px;background-color:rgba(250, 114, 152, 0.25);border-radius:15px;");
                })
                .catch(function (err) {
                    console.log('%c PWA Service Worker 注册失败 ',"margin:5px;color:#F05B85;padding:5px;background-color:rgba(250, 114, 152, 0.25);border-radius:15px;");
                });
         });
    }
</script>
<% } %>